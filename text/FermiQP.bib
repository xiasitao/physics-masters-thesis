  author = {Cheuk, Lawrence W. and Nichols, Matthew A. and Okan, Melih and Gersdorf, Thomas and Ramasesh, Vinay V. and Bakr, Waseem S. and Lompe, Thomas and Zwierlein, Martin W.},
  date = {2015-05-13},
