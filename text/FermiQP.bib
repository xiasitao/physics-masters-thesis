  langid = {english},
  keywords = {Condensed Matter - Quantum Gases,Quantum Physics},
  file = {/home/maxi/Masterarbeit/Zotero/Kwon et al_2022_Site-Resolved Imaging of Bosonic Mott Insulator of $^7$Li atoms.pdf}
